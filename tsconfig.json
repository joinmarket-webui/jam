{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "allowJs": false,
    "resolveJsonModule": true,
    "noUncheckedIndexedAccess": true
  }
}

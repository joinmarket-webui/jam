{
  "name": "react-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "test": "vitest run",
    "regtest:rebuild": "docker-compose -f docker/regtest/docker-compose.yml build --no-cache",
    "regtest:up": "cd docker/regtest && ./prepare-setup.sh && docker-compose up -d",
    "regtest:restart": "docker-compose -f docker/regtest/docker-compose.yml restart joinmarket",
    "regtest:init": "cd docker/regtest && ./init-setup.sh",
    "regtest:mine": "cd docker/regtest && ./mine-block.sh",
    "regtest:fund": "cd docker/regtest && ./fund-wallet.sh",
    "regtest:down": "docker-compose -f docker/regtest/docker-compose.yml down",
    "regtest:clear": "docker-compose -f docker/regtest/docker-compose.yml down -v",
    "regtest:logs": "docker-compose -f docker/regtest/docker-compose.yml logs -f",
    "regtest:status": "docker-compose -f docker/regtest/docker-compose.yml ps"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/react-query": "^5.80.6",
    "@tanstack/react-query-devtools": "^5.80.6",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.1",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0",
    "@eslint/js": "^9.25.0",
    "@storybook/addon-a11y": "^9.0.3",
    "@storybook/addon-docs": "^9.0.3",
    "@storybook/addon-onboarding": "^9.0.3",
    "@storybook/addon-vitest": "^9.0.3",
    "@storybook/react-vite": "^9.0.3",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^9.0.3",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "playwright": "^1.52.0",
    "storybook": "^9.0.3",
    "tw-animate-css": "^1.3.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  }
}
